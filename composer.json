{
    "name": "mrunknown0001/laravel-login-monitor",
    "description": "Laravel package for monitoring login activities and sending beacons to a monitoring host",
    "keywords": ["laravel", "login", "monitoring", "security", "authentication", "beacon"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Mrunknown0001",
            "email": "madamt0001@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1|^8.2|^8.3|^8.4",
        "illuminate/support": "^9.0|^10.0|^11.0|^12.0",
        "illuminate/http": "^9.0|^10.0|^11.0|^12.0",
        "illuminate/queue": "^9.0|^10.0|^11.0|^12.0"
    },
    "autoload": {
        "psr-4": {
            "Mrunknown0001\\LaravelLoginMonitor\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Mrunknown0001\\LaravelLoginMonitor\\ActivityLoggerServiceProvider"
            ],
            "aliases": {
                "ActivityLogger": "Mrunknown0001\\LaravelLoginMonitor\\Facades\\ActivityLogger"
            }
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require-dev": {
        "laravel/framework": "^12.0",
        "orchestra/testbench": "^9.0",
        "mockery/mockery": "^1.5"
    }
}
