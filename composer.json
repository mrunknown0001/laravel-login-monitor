{
    "name": "mrunknown0001/laravel-login-monitor",
    "description": "Laravel package for monitoring login activities and sending beacons to a monitoring host",
    "keywords": ["laravel", "login", "monitoring", "security", "authentication", "beacon"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Your Name",
            "email": "your.email@example.com"
        }
    ],
    "require": {
        "php": "^8.1|^8.2|^8.3|^8.4",
        "illuminate/support": "^9.0|^10.0|^11.0|^12.0",
        "illuminate/http": "^9.0|^10.0|^11.0|^12.0"
    },
    "autoload": {
        "psr-4": {
            "Mrunknown0001\\LaravelLoginMonitor\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Mrunknown0001\\LaravelLoginMonitor\\LoginMonitorServiceProvider"
            ],
            "aliases": {
                "LoginMonitor": "Mrunknown0001\\LaravelLoginMonitor\\Facades\\LoginMonitor"
            }
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}